{
  "name": "testAutoCloud",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/console": "^1.0.0",
    "@sap/cds": "^9",
    "cds-swagger-ui-express": "^0.10.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve",
    "run": "cds watch --profile development",
    "build": "cds build && cds build --production",
    "deploy": "cds deploy --to sqlite"
  },
  "private": true,
  "cds": {
    "requires": {
      "db-ext": {
        "[development]": {
          "model": "db/sqlite"
        },
        "[production]": {
          "model": "db/hana"
        }
      },
      "destinations": true,
      "connectivity": true
    },
    "protocols": {
      "rest": {
        "path": "/rest"
      }
    },
    "openapi": {
      "swagger_ui": {
        "url": "/swagger"
      }
    }
  },
  "type": "module",
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
